dbh:utdanningsdata a dcat:Dataset;
    dct:title "Statistikk om høyere utdanning"@nb ;
    dct:description "Database for statistikk om høyere utdanning (DBH) inneholder informasjon om hele den tertiære utdanningen i Norge som inkluderer universiteter, høyskoler og høyere yrkesfaglig utdanning."@nb ;
    dct:publisher <https://dbh.hkdir.no/> ;
    dcat:theme theme:POP ;
    dct:spatial loc:NO ;
    dct:license <https://creativecommons.org/licenses/by/4.0/> ;
    dcat:contactPoint ssb:kontaktpunkt ;
    dcat:accessURL https://dbh.hkdir.no/tall-og-statistikk/statistikk-meny/.

dbh:kontaktpunkt a vcard:Kind ;
    vcard:fn "Database for statistikk om høyere utdanning"@nb ;
    vcard:hasEmail <mailto:dbh@hkdir.no> ;
    vcard:hasURL <https://dbh.hkdir.no/about/contact> .

loc:NO a dct:Location ;
    skos:prefLabel "Norge"@nb ;
    dct:identifier "NO"^^xsd:string .


